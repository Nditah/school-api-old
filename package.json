{
  "name": "school-api",
  "version": "1.0.0",
  "description": "Winning API",
  "author": "Nditah Sam",
  "license": "MIT",
  "homepage": "https://github.com/Nditah/school-api#readme",
  "main": "dist",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "start": "NODE_ENV=development nodemon --exec babel-node --presets=latest -- ./src",
    "serve": "NODE_ENV=production node ./dist",
    "apprunner": "pm2 start ./dist -i 4 --env production",
    "mocha": "mocha --timeout 10000 --compilers js:babel-register",
    "test": "NODE_ENV=test mocha --compilers --no-deprecation js:babel-register --recursive ./test/",
    "kill": "sudo kill $(sudo lsof -t -i:5000)",
    "doc": "rm -rf dist/public && apidoc -i src/ -o dist/public",
    "dist": "babel src -s -D -d dist && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nditah/school-api.git"
  },
  "keywords": [
    "Express",
    "RestAPI",
    "MongoDB"
  ],
  "dependencies": {
    "api-query-params": "^4.9.0",
    "app-root-path": "^2.1.0",
    "aws-sdk": "^2.378.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "express": "^4.16.3",
    "express-force-ssl": "^0.3.2",
    "fast-csv": "^2.5.0",
    "helmet": "^3.13.0",
    "joi": "^14.3.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.15",
    "log4js": "^3.0.6",
    "moment": "^2.24.0",
    "mongoose": "^5.4.7",
    "mongoose-csv": "^1.1.0",
    "morgan": "^1.9.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "neo-async": "^2.6.0",
    "pm2": "^3.1.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "slugify": "^1.3.4",
    "socket.io": "^2.2.0",
    "underscore": "^1.9.1",
    "unirest": "^0.6.0",
    "util": "^0.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "apidoc": "^0.17.6",
    "apidoc-markdown2": "^0.2.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "coffeescript": "^2.3.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.6",
    "opn-cli": "^3.1.0"
  }
}
